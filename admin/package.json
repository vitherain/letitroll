{
  "name": "admin",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "format:check":
      "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix": "pretty-quick --staged",
    "format:cli-fix": "pretty-quick",
    "precommit": "run-s format:fix lint",
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json",
    "build": "ng build --prod --environment=prod",
    "aot": "ng build --prod --aot --environment=prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/cdk": "^5.2.4",
